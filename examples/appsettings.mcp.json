{
  "A3sist": {
    "Agents": {
      "Orchestrator": {
        "Enabled": true,
        "MaxConcurrentTasks": 10,
        "Timeout": "00:05:00"
      },
      "MaxConcurrentAgents": 15,
      "HealthCheckInterval": "00:00:30",
      "CircuitBreakerThreshold": 5
    },
    "LLM": {
      "Provider": "OpenAI",
      "Model": "gpt-4",
      "MaxTokens": 4000,
      "ApiEndpoint": "https://api.openai.com/v1",
      "ApiKey": "your-api-key-here",
      "RequestTimeout": "00:02:00",
      "EnableCaching": true,
      "CacheExpiration": "01:00:00",
      "MCP": {
        "Enabled": true,
        "Version": "1.0",
        "EnableTools": true,
        "EnableOrchestration": true,
        "MaxToolExecutions": 10,
        "ToolTimeout": "00:00:30",
        "HealthCheckInterval": "00:05:00",
        "FallbackProviders": [
          "OpenAI",
          "Anthropic",
          "Codestral"
        ],
        "EnableFailover": true,
        "RetryPolicy": {
          "MaxRetries": 3,
          "InitialDelay": "00:00:00.500",
          "MaxDelay": "00:00:10",
          "UseExponentialBackoff": true
        },
        "Servers": {
          "CoreDevelopment": {
            "Enabled": true,
            "Endpoint": "http://localhost:3001",
            "Tools": [
              "code_analysis",
              "code_refactor",
              "code_validation",
              "language_conversion"
            ],
            "Timeout": "00:00:30",
            "Priority": 10,
            "EnableHealthCheck": true
          },
          "VSIntegration": {
            "Enabled": true,
            "Endpoint": "http://localhost:3002",
            "Tools": [
              "project_analysis",
              "solution_management",
              "nuget_operations",
              "msbuild_operations",
              "extension_integration"
            ],
            "Timeout": "00:00:45",
            "Priority": 9,
            "EnableHealthCheck": true
          },
          "Knowledge": {
            "Enabled": true,
            "Endpoint": "http://localhost:3003",
            "Tools": [
              "documentation_search",
              "best_practices",
              "code_examples",
              "knowledge_update"
            ],
            "Timeout": "00:00:20",
            "Priority": 8,
            "EnableHealthCheck": true
          },
          "GitDevOps": {
            "Enabled": true,
            "Endpoint": "http://localhost:3004",
            "Tools": [
              "git_operations",
              "ci_cd_integration",
              "deployment_analysis"
            ],
            "Timeout": "00:00:35",
            "Priority": 7,
            "EnableHealthCheck": true
          },
          "TestingQuality": {
            "Enabled": true,
            "Endpoint": "http://localhost:3005",
            "Tools": [
              "test_generation",
              "quality_metrics",
              "performance_analysis"
            ],
            "Timeout": "00:00:40",
            "Priority": 6,
            "EnableHealthCheck": true
          }
        }
      }
    },
    "Logging": {
      "Level": "Information",
      "OutputPath": "%TEMP%/A3sist/logs",
      "EnableFileLogging": true,
      "EnableConsoleLogging": true,
      "EnableStructuredLogging": true
    },
    "Performance": {
      "EnableMonitoring": true,
      "MetricsInterval": "00:01:00",
      "MaxMemoryUsageMB": 1024,
      "EnableAutoGC": false
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "A3sist.Core.LLM": "Debug",
        "A3sist.Core.Agents": "Debug"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "%TEMP%/A3sist/logs/a3sist-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Level:u3}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}